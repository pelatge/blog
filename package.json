{
  "name": "nusendra-blog",
  "description": "Nusendras blog built with Sapper - Svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "watch:css": "postcss src/assets/global.css -o static/global.css -w",
    "build": "sapper build --legacy",
    "build:css": "NODE_ENV=production postcss src/assets/global.css -o static/global.css",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^0.5.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "autoprefixer": "^10.1.0",
    "cssnano": "^4.1.10",
    "date-fns": "^1.30.1",
    "gray-matter": "^4.0.2",
    "marked": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-load-config": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.0.3",
    "prismjs": "^1.21.0",
    "reading-time": "^1.2.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.11",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2"
  }
}
